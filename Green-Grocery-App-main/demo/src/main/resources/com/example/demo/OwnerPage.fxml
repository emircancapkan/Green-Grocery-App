<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="540.0" prefWidth="960.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.OwnerPageController">
   <BorderPane prefHeight="540.0" prefWidth="960.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <left>
         <VBox alignment="CENTER" fillWidth="false" prefHeight="540.0" prefWidth="142.0" spacing="20.0" BorderPane.alignment="CENTER">
            <children>
                    <Button alignment="CENTER" contentDisplay="CENTER" onAction="#addremoveupdateProduct" prefHeight="50.0" prefWidth="120.0" style="-fx-background-color: purple;" styleClass="mainPageButtons" stylesheets="@styles.css" text="Products" textFill="#a90a84">
                  <font>
                     <Font name="System Bold" size="20.0" />
                  </font>
                  <cursor>
                     <Cursor fx:constant="HAND" />
                  </cursor>
                  <VBox.margin>
                     <Insets top="150.0" />
                  </VBox.margin></Button>
                    <Button alignment="CENTER" contentDisplay="CENTER" onAction="#employfireCarrier" prefHeight="50.0" prefWidth="120.0" style="-fx-background-color: purple;" styleClass="mainPageButtons" stylesheets="@styles.css" text="Carrier" textFill="#a90a84">
                  <font>
                     <Font name="System Bold" size="20.0" />
                  </font>
                  <cursor>
                     <Cursor fx:constant="HAND" />
                  </cursor></Button>
                    <Button alignment="CENTER" contentDisplay="CENTER" onAction="#viewAllOrders" prefHeight="50.0" prefWidth="120.0" style="-fx-background-color: purple;" styleClass="mainPageButtons" stylesheets="@styles.css" text="Orders" textFill="#a90a84">
                  <font>
                     <Font name="System Bold" size="20.0" />
                  </font>
                  <cursor>
                     <Cursor fx:constant="HAND" />
                  </cursor></Button>
               <Button mnemonicParsing="false" onAction="#back" prefHeight="34.0" prefWidth="60.0" style="-fx-background-color: purple;" styleClass="mainPageButtons" stylesheets="@styles.css" text="Logout">
                  <font>
                     <Font name="System Bold" size="12.0" />
                  </font>
                  <VBox.margin>
                     <Insets top="100.0" />
                  </VBox.margin>
                  <cursor>
                     <Cursor fx:constant="HAND" />
                  </cursor>
               </Button>
            </children>
         </VBox>
      </left>
      <center>
         <VBox id="OwnerPageBackground" fx:id="m" alignment="CENTER" fillWidth="false" prefHeight="653.0" prefWidth="790.0" stylesheets="@styles.css" BorderPane.alignment="CENTER">
            <children>
               <AnchorPane fx:id="MenuAnchorPane" prefHeight="457.0" prefWidth="656.0" />
            </children>
         </VBox>
      </center>
   </BorderPane>
